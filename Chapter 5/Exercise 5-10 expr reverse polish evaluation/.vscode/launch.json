    "version": "0.2.0",
    "configurations": [
          {
      "name": "Debug Test.exe",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/Test.exe",
      "args": ["3", "4", "+", "$p"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "preLaunchTask": "C/C++: gcc.exe build active file"
    }
    ]
}